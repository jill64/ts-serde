    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/ts-serde/issues",
  "homepage": "https://github.com/jill64/ts-serde#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./object": {
      "types": "./dist/object/index.d.ts",
      "default": "./dist/object/index.js"
    },
    "./primitive": {
      "types": "./dist/primitive/index.d.ts",
      "default": "./dist/primitive/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/ts-serde.git",
    "image": "https://opengraph.githubassets.com/89168c7ac240cf603533efdc0fdfc1c3802f7a732d620bd479962cad04cb8fb2/jill64/ts-serde"
  },
  "keywords": [
    "deserialize",
    "serde",
    "serialize",
    "type-safe"
  ],
  "scripts": {
    "build": "tsc && bunx publint",
    "prepack": "bun run build",
    "lint": "bunx eslint . && bunx depcheck",
    "format": "bunx prettier -w .",
    "test": "vitest"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@jill64/eslint-config-ts": "2.0.17",
    "@jill64/prettier-config": "1.0.0",
    "typescript": "5.8.2",
    "vitest": "3.0.8"
  },
  "dependencies": {
    "devalue": "5.1.1"
  }
}
